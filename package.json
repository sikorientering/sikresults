{
  "name": "sikresults",
  "private": true,
  "version": "0.0.1",
  "description": "SIK Results Site",
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "lint": "npm run lint:src && npm run lint:scss",
    "lint:fix": "npm run lint:src:fix && npm run lint:scss:fix",
    "lint:prod": "npm run lint:src:prod && npm run lint:scss:prod",
    "lint:src": "eslint \"**/*.{ts,tsx,js}\" --max-warnings=0 && prettier -l \"{!(package*).json,.prettierrc}\"",
    "lint:src:fix": "eslint \"**/*.{ts,tsx,js}\" --fix --max-warnings=0 && prettier --write \"{!(package*).json,.prettierrc}\"",
    "lint:src:prod": "eslint \"**/*.{ts,tsx,js}\" --rule \"linebreak-style: [error, unix]\" --max-warnings=0 && prettier -l \"{!(package*).json,.prettierrc}\"",
    "lint:scss": "prettier -c --ignore-path ./.gitignore **/*.scss",
    "lint:scss:fix": "prettier --write --ignore-path ./.gitignore **/*.scss",
    "lint:scss:prod": "prettier -c --end-of-line lf --ignore-path ./.gitignore **/*.scss",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^2.5.2",
    "@stencil/router": "^1.0.1",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.4.1",
    "@types/jest": "^26.0.20",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "prettier": "^2.2.1",
    "puppeteer": "2.1.1",
    "typescript": "^4.2.3",
    "lodash-es": "^4.17.21",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "license": "MIT",
  "dependencies": {
    "@limetech/lime-elements": "^31.4.1"
  }
}
